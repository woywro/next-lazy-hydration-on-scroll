{
  "name": "next-lazy-hydration-on-scroll",
  "version": "1.1.1",
  "description": "Hydrate components dynamically as the user scrolls.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "author": {
    "name": "Wojciech Wrotek",
    "email": "wojtek@wrotek.pl"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
