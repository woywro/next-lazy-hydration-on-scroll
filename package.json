    "@semantic-release/git": "^10.0.1",
    "@types/node": "^17.0.45",
    "@types/react": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "tsup": "^8.2.4",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "next": ">=12",
    "react": ">=18"
  }
